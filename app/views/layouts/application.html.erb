<!DOCTYPE html>
<html>
  <head>
    <title>Kieran</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <div class="outer">
        <div class="empty" id="empty_left"></div>
        <div class="center">
            <div class="header">
                <%= render "shared/header" %>
            </div>
            <div class="content">
                <%= yield %>
            </div>
        </div>
        <div class="empty" id="empty_right"></div>
    </div>
  </body>
</html>
